{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/landing.html"
      },
      {
        "source": "/landing",
        "destination": "/landing.html"
      },
      {
        "source": "/login",
        "destination": "/login.html"
      },
      {
        "source": "/signup",
        "destination": "/signup.html"
      },
      {
        "source": "/home",
        "destination": "/home.html"
      },
      {
        "source": "/user",
        "destination": "/user.html"
      },
      {
        "source": "/profile",
        "destination": "/EditProfile.html"
      }
    ]
  }
}
